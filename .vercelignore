# Vercel ignore file
# Exclude development and test files

# Node modules (will be installed by Vercel)
node_modules/
*/node_modules/

# Test files
**/__tests__/
**/*.test.js
**/*.test.ts
**/*.test.tsx
jest.config.js

# Development files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# IDE files
.vscode/
.idea/
*.swp
*.swo

# OS files
.DS_Store
Thumbs.db

# Git
.git/
.gitignore

# Documentation (optional)
README.md
DEPLOYMENT_GUIDE.md
ENVIRONMENT_VARIABLES.md

# Backup files
*.backup
*.bak
