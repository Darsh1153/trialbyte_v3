{
  "version": 2,
  "builds": [
    {
      "src": "trialbyte-frontend-v1/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "trialbyte-backend-v1/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "trialbyte-backend-v1/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "trialbyte-frontend-v1/$1"
    }
  ]
}
